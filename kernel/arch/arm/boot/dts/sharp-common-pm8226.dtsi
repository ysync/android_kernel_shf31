		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
//		qcom,hw-settle-time = <0>;
//		qcom,fast-avg-setup = <0>;
	};

	chan@1 { /* external_rsense */
	};
};

/*****************************************/
/*  SHARP Device Drivers                 */
/*****************************************/

&soc {
	/*---------------------------------------*/
	/*  Battery Control Driver               */
	/*---------------------------------------*/
	sharp,shbatt {
		compatible = "sharp,shbatt";
		status = "ok";
	};

	/*---------------------------------------*/
	/*  Charger Control Driver               */
	/*---------------------------------------*/
	sharp,shchg {
		compatible = "sharp,shchg";
		status = "ok";
	};
};

&spmi_bus {
/*****************************************/
/*  qpnp-resin                           */
/*****************************************/
	qcom,pm8226@0 {
		qcom,power-on@800 {
			qcom,s3-debounce = <16>;
			qcom,s3-src = "kpdpwr";

			qcom,pon_1 {
				qcom,support-reset = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <1000>;
				qcom,s2-type = <7>;
			};

			qcom,pon_2 {
				qcom,support-reset = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <1000>;
				qcom,s2-type = <7>;
			};

			qcom,pon_3 {
				qcom,support-reset = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <1000>;
				qcom,s2-type = <7>;
			};
		};
	};

/*****************************************/
/*  Disable WLED setting                 */
/*****************************************/
    qcom,pm8226@1 {
        qcom,leds@d800 {
            status = "disabled";
        };
    };
};
