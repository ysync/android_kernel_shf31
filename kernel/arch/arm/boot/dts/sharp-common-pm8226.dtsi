/*****************************************/
/*  qpnp-charger                         */
/*****************************************/
&pm8226_chg {
	qcom,bat-if@1200 {
		status = "ok";
	};
};

/*****************************************/
/*  qpnp-bms                             */
/*****************************************/

&pm8226_bms {
	status = "ok";
	qcom,use-external-rsense;

	qcom,bms-iadc@3800 {
		status = "ok";
	};

	qcom,bms-bms@4000 {
		status = "ok";
	};
};

/*****************************************/
/*  qpnp-adc-voltage                     */
/*****************************************/
&pm8226_vadc {
	chan@1 { /* dc_in */
		label = "dc_in";
		reg = <1>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <4>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <0>;
		qcom,fast-avg-setup = <0>;
	};

	chan@6 { /* vbat_sns */
//		qcom,decimation = <0>;
//		qcom,pre-div-channel-scaling = <1>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
//		qcom,hw-settle-time = <0>;
//		qcom,fast-avg-setup = <0>;
		sharp,scale-function = <6>;
	};

	chan@b { /* chg_temp */
		label = "chg_temp";
		reg = <0xb>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <3>;
		qcom,hw-settle-time = <0>;
		qcom,fast-avg-setup = <0>;
	};

	chan@e { /* gnd_ref */
		label = "gnd_ref";
		reg = <0xe>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <0>;
		qcom,fast-avg-setup = <0>;
	};

	chan@f { /* vdd_vadc */
		label = "vdd_vadc";
		reg = <0xf>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <0>;
		qcom,fast-avg-setup = <0>;
	};

	chan@13 { /* mux4_1_1(mpp4) -> usb_id_pu1 */
		label = "usb_id_pu1";
		reg = <0x13>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@14 { /* mux5_1_1(mpp5) -> pa_therm0 */
		label = "pa_therm0";
		reg = <0x14>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};
	
	chan@15 { /* mux6_1_1(mpp6) -> msm_therm */
		label = "msm_therm";
		reg = <0x15>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@21 { /* mux2_1_3(mpp2) -> chg_cur_out */
		label = "chg_cur_out";
		reg = <0x21>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <1>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@b7 { /* pa_therm1 */
		label = "pa_therm1";
		reg = <0xb7>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <2>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@b8 { /* quiet_therm -> lcd_therm */
		label = "lcd_therm";
		reg = <0xb8>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
		sharp,scale-function = <4>;
	};
};

