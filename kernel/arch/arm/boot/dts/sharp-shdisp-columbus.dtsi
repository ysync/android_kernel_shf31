&soc {
	/*---------------------*/
	/* LCD controller      */
	/*---------------------*/
	sharp,shdisp_columbus {
		compatible = "sharp,shdisp_columbus";
		status = "disable";
		interrupts = <24 0>;
		interrupt-parent = <&msmgpio>;
		vdd-supply = <&pm8226_l23>;
		vddio-supply = <&pm8226_l6>;
	};
};
